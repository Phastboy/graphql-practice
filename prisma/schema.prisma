generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
      url      = env("DATABASE_URL")
}

model Book {
    id        Int      @id @default(autoincrement())
      title     String
        author    String
          reviews   Review[]
}

model Review {
    id        Int      @id @default(autoincrement())
      content   String
        rating    Int
          book      Book     @relation(fields: [bookId], references: [id])
            bookId    Int
}
